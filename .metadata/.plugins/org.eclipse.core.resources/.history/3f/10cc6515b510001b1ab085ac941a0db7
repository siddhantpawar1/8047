package Java_Learning;

public class SwitchStatement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x=0;
		switch(x)
		{
		
		case 1:
			System.out.println("Performance is poor");
			break;
		case 2:
			System.out.println("Slow performance");
			break;
			
		case 3:
			System.out.println("Meering Expectation");
			break;
			
		case 4:
			System.out.println("Well done keep rocking");
			break;
			
		case 5:
			System.out.println("Employee of the year");
			break;
			
		default:
			System.out.println("You donot have any output");
		}
		}

	}


