package Java_Learning;

public class methoddemo2 {

	public static void main(String[] args) {
		
		System.out.println("Begins execution of the program....");
		
		methoddemo2 MD = new methoddemo2();
		MD.sum();
		MD.sub();
		
		System.out.println("This ends the program execution!!!");
			
		
	}
		
		public void sum()
		
		{
			System.out.println("It performs addition");
			
		}
		
		
		public void sub() {
			
			System.out.println("It performs subtracting");
			
		}

	}
