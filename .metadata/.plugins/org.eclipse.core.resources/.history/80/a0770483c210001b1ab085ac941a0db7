package Java_Learning;

public class Globalvar_5 {

	int age;
	double marks;
	
	public void Instancevariable(int roll, double marks) {
	this.age = age;
	this.marks = marks;
		
		
	}
	
	public static void main(String[] args) {
		
		Globalvar_5 obj1 = new Globalvar_5();
		Globalvar_5 obj1 = new Globalvar_5();
		Globalvar_5 obj1 = new Globalvar_5();
		
	}	
		

	}


