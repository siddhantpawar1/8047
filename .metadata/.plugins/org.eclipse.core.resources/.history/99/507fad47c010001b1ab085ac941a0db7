package Java_Learning;

public class LocalGlobalVar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x=12;
		System.out.println("Print the value of X: "+x);
		
	}
		
	public void sid() {
		
	for(int i=0;i<=20;i++)
		
		System.out.println("The value of 1 is: " +i);
		
	}
	
	
	public void piyu() {
		
		System.out.println("Print the value of x" +x);
		
	}

	}


