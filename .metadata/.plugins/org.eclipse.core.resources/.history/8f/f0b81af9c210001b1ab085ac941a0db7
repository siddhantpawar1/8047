package Java_Learning;

public class Globalvar_5 {

	int age;
	double marks;
	
	public void Instancevariable(int roll, double marks) {
	this.age = age;
	this.marks = marks;
		
		
	}
	
	public static void main(String[] args) {
		
		Globalvar_5 obj1 = new Globalvar_5(15,80.25);
		Globalvar_5 obj2 = new Globalvar_5(20, 90.65);
		Globalvar_5 obj3 = new Globalvar_5(30, 95.68);
		
	}	
		
	public void show() {
		
		System.out.println("age is: "+age+ "  " +"marks is: "+marks);
		
	}
	

	}


