package javedemo;


public class loopdemo2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
int numb = 1;		
for(int i=1;i<5 ;i++) {
						
for(int j=1;j<=i;j++) {
				System.out.print(numb);
				System.out.print("\t");
				numb++;
			}
System.out.println("");
		}
	

	}
}