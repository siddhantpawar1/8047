package Java_Learning;

public class ifelsedemo {

	public static void main(String[] args) {
		
		
		System.out.println("Start Testing");
		
		int a=50;
		
		
		if(a<100) {
		System.out.println("Value of first if block is greater:" +a);
	    }
		if(a<40) {
		System.out.println("value of second if block is greater: " +a);
		}
	
	}

}
